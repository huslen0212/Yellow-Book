generator client {
  provider = "prisma-client-js"
  output   = "../generated/prisma"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model place {
  id                Int     @id @default(autoincrement())
  name              String
  founded_year      Int?
  image_url         String?
  short_description String?
  long_description  String?
  location          String?
  phone_number      String?
  facebook_url      String?
  instagram_url     String?
  website_url       String?
  category          String?
}
